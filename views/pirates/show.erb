<h1>Display your Pirate here</h1>

<div class="Pirate">
  <h3>Name: <%= @pirate.name %></h3><br>
  <h4>Grade: <%= @pirate.weight %></h4>
</div><br>

<h1>Courses</h1>
<% @courses.each do |course| %>
  <div class="course">
    <p>Name: <%= course.name %></p><br>
    <p>Topic: <%= course.topic %></p><br>
  </div><br>
<% end %>


